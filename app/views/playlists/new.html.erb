<%= form_for [@user, @playlist] do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :songs %>
  <%= f.select :songs, options_from_collection_for_select(@user.songs, :id, :title), {}, { multiple: true } %>

 <%= f.label :users %>
  <%= f.select :users, options_from_collection_for_select(@users, :first_name), {}, { multiple: true } %>

  <%= f.submit "Create" %>
<% end %>